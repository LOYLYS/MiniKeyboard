<Keyboard xmlns:app="http://schemas.android.com/apk/res-auto"
    app:keyWidth="9%p"
    app:horizontalGap="1%p"
    app:verticalGap="10dp"
    app:keyHeight="50dp">

    <Row>
        <Key
            app:codes="49"
            app:keyEdgeFlags="left"
            app:keyLabel="1" />
        <Key
            app:codes="50"
            app:keyLabel="2" />
        <Key
            app:codes="51"
            app:keyLabel="3" />
        <Key
            app:codes="52"
            app:keyLabel="4" />
        <Key
            app:codes="53"
            app:keyLabel="5" />
        <Key
            app:codes="54"
            app:keyLabel="6" />
        <Key
            app:codes="55"
            app:keyLabel="7" />
        <Key
            app:codes="56"
            app:keyLabel="8" />
        <Key
            app:codes="57"
            app:keyLabel="9" />
        <Key
            app:codes="48"
            app:keyEdgeFlags="right"
            app:keyLabel="0" />
    </Row>

    <Row>
        <Key
            app:codes="64"
            app:keyEdgeFlags="left"
            app:keyLabel="\@" />
        <Key
            app:codes="35"
            app:keyLabel="\#" />
        <Key
            app:codes="36"
            app:keyLabel="$" />
        <Key
            app:codes="37"
            app:keyLabel="%" />
        <Key
            app:codes="38"
            app:keyLabel="&amp;" />
        <Key
            app:codes="42"
            app:keyLabel="*" />
        <Key
            app:codes="45"
            app:keyLabel="-" />
        <Key
            app:codes="61"
            app:keyLabel="=" />
        <Key
            app:codes="40"
            app:keyLabel="(" />
        <Key
            app:codes="41"
            app:keyEdgeFlags="right"
            app:keyLabel=")" />
    </Row>

    <Row>
        <Key
            app:codes="-1"
            app:isModifier="true"
            app:isSticky="true"
            app:keyEdgeFlags="left"
            app:keyIcon="@drawable/sym_keyboard_shift"
            app:keyWidth="15%p" />
        <Key
            app:codes="33"
            app:keyLabel="!" />
        <Key
            app:codes="34"
            app:keyLabel="&quot;" />
        <Key
            app:codes="39"
            app:keyLabel="\'" />
        <Key
            app:codes="58"
            app:keyLabel=":" />
        <Key
            app:codes="59"
            app:keyLabel=";" />
        <Key
            app:codes="47"
            app:keyLabel="/" />
        <Key
            app:codes="63"
            app:keyLabel="\?" />
        <Key
            app:codes="-5"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/ic_key_backsapce"
            app:keyWidth="15%p" />
    </Row>

    <Row app:rowEdgeFlags="bottom">
        <Key
            app:codes="-3"
            app:keyEdgeFlags="left"
            app:keyIcon="@drawable/sym_keyboard_done"
            app:keyWidth="15%p" />
        <Key
            app:codes="-2"
            app:keyLabel="ABC"
            app:keyWidth="10%p" />
        <Key
            app:codes="-101"
            app:keyIcon="@drawable/sym_keyboard_language_switch"
            app:keyWidth="10%p" />
        <Key
            app:codes="32"
            app:isRepeatable="true"
            app:keyWidth="30%p" />
        <Key
            app:codes="44"
            app:keyLabel=","
            app:keyWidth="15%p" />
        <Key
            app:codes="10"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/ic_key_enter"
            app:keyWidth="20%p" />
    </Row>
</Keyboard>