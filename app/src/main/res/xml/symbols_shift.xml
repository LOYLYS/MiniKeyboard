<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:app="http://schemas.android.com/apk/res-auto"
    app:horizontalGap="1%p"
    app:keyHeight="@dimen/key_height_symbol"
    app:keyWidth="8.8%p"
    app:verticalGap="@dimen/key_vertical_gap_symbol">

    <Row>
        <Key
            app:codes="64"
            app:keyEdgeFlags="left"
            app:keyLabel="\@" />
        <Key
            app:codes="35"
            app:keyLabel="\#" />
        <Key
            app:codes="38"
            app:keyLabel="&amp;" />
        <Key
            app:codes="63"
            app:keyLabel="\?" />
        <Key
            app:codes="191"
            app:keyLabel="¿" />
        <Key
            app:codes="33"
            app:keyLabel="!" />
        <Key
            app:codes="161"
            app:keyLabel="¡" />
        <Key
            app:codes="188"
            app:keyLabel="¼" />
        <Key
            app:codes="189"
            app:keyLabel="½" />
        <Key
            app:codes="190"
            app:keyEdgeFlags="right"
            app:keyLabel="¾" />
    </Row>

    <Row>
        <Key
            app:codes="36"
            app:keyEdgeFlags="left"
            app:keyLabel="$" />
        <Key
            app:codes="163"
            app:keyLabel="£" />
        <Key
            app:codes="8364"
            app:keyLabel="€" />
        <Key
            app:codes="165"
            app:keyLabel="¥" />
        <Key
            app:codes="92"
            app:keyLabel="\\" />
        <Key
            app:codes="124"
            app:keyLabel="|" />
        <Key
            app:codes="94"
            app:keyLabel="^" />
        <Key
            app:codes="95"
            app:keyLabel="_" />
        <Key
            app:codes="34"
            app:keyLabel="&quot;" />
        <Key
            app:codes="59"
            app:keyEdgeFlags="right"
            app:keyLabel=";" />
    </Row>

    <Row>
        <Key
            app:codes="-1"
            app:isModifier="true"
            app:isSticky="true"
            app:keyEdgeFlags="left"
            app:keyIcon="@drawable/ic_key_shift"
            app:keyWidth="13%p" />
        <Key
            app:codes="171"
            app:keyLabel="«" />
        <Key
            app:codes="8226"
            app:keyLabel="•" />
        <Key
            app:codes="176"
            app:keyLabel="°" />
        <Key
            app:codes="96"
            app:keyLabel="`" />
        <Key
            app:codes="39"
            app:keyLabel="\'" />
        <Key
            app:codes="180"
            app:keyLabel="´" />
        <Key
            app:codes="187"
            app:keyLabel="»" />
        <Key
            app:codes="-5"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/ic_key_backspace"
            app:keyWidth="14.3%p" />
    </Row>

    <Row
        app:keyHeight="@dimen/key_height_default"
        app:rowEdgeFlags="bottom">
        <Key
            app:codes="-2,-2"
            app:keyEdgeFlags="left"
            app:keyLabel="abc"
            app:keyWidth="13%p" />
        <Key
            app:codes="60"
            app:keyLabel="&lt;" />
        <Key
            app:codes="-101"
            app:keyIcon="@drawable/ic_key_language" />
        <Key
            app:codes="32"
            app:isRepeatable="true"
            app:keyWidth="38.4%p" />
        <Key
            app:codes="62"
            app:keyLabel="&gt;" />
        <Key
            app:codes="10"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/ic_key_enter"
            app:keyWidth="14.3%p" />
    </Row>
</Keyboard>
