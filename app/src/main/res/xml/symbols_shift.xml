<Keyboard xmlns:app="http://schemas.android.com/apk/res-auto"
    app:keyWidth="9%p"
    app:horizontalGap="1%p"
    app:verticalGap="10dp"
    app:keyHeight="50dp">

    <Row>
        <Key
            app:codes="126"
            app:keyEdgeFlags="left"
            app:keyLabel="~" />
        <Key
            app:codes="177"
            app:keyLabel="±" />
        <Key
            app:codes="215"
            app:keyLabel="×" />
        <Key
            app:codes="247"
            app:keyLabel="÷" />
        <Key
            app:codes="8226"
            app:keyLabel="•" />
        <Key
            app:codes="176"
            app:keyLabel="°" />
        <Key
            app:codes="96"
            app:keyLabel="`" />
        <Key
            app:codes="180"
            app:keyLabel="´" />
        <Key
            app:codes="123"
            app:keyLabel="{" />
        <Key
            app:codes="125"
            app:keyEdgeFlags="right"
            app:keyLabel="}" />
    </Row>

    <Row>
        <Key
            app:codes="169"
            app:keyEdgeFlags="left"
            app:keyLabel="©" />
        <Key
            app:codes="163"
            app:keyLabel="£" />
        <Key
            app:codes="8364"
            app:keyLabel="€" />
        <Key
            app:codes="94"
            app:keyLabel="^" />
        <Key
            app:codes="174"
            app:keyLabel="®" />
        <Key
            app:codes="165"
            app:keyLabel="¥" />
        <Key
            app:codes="95"
            app:keyLabel="_" />
        <Key
            app:codes="43"
            app:keyLabel="+" />
        <Key
            app:codes="91"
            app:keyLabel="[" />
        <Key
            app:codes="93"
            app:keyEdgeFlags="right"
            app:keyLabel="]" />
    </Row>

    <Row>
        <Key
            app:codes="-1"
            app:isModifier="true"
            app:isSticky="true"
            app:keyEdgeFlags="left"
            app:keyIcon="@drawable/sym_keyboard_shift"
            app:keyWidth="15%p" />
        <Key
            app:codes="161"
            app:keyLabel="¡" />
        <Key
            app:codes="60"
            app:keyLabel="&lt;" />
        <Key
            app:codes="62"
            app:keyLabel="&gt;" />
        <Key
            app:codes="162"
            app:keyLabel="¢" />
        <Key
            app:codes="124"
            app:keyLabel="|" />
        <Key
            app:codes="92"
            app:keyLabel="\\" />
        <Key
            app:codes="191"
            app:keyLabel="¿" />
        <Key
            app:codes="-5"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_delete"
            app:keyWidth="15%p" />
    </Row>

    <Row app:rowEdgeFlags="bottom">
        <Key
            app:codes="-3"
            app:keyEdgeFlags="left"
            app:keyIcon="@drawable/sym_keyboard_done"
            app:keyWidth="15%p" />
        <Key
            app:codes="-2"
            app:keyLabel="ABC"
            app:keyWidth="10%p" />
        <!--
            app:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->
        <Key
            app:codes="-101"
            app:keyIcon="@drawable/sym_keyboard_language_switch"
            app:keyWidth="10%p" />
        <Key
            app:codes="32"
            app:isRepeatable="true"
            app:keyIcon="@drawable/sym_keyboard_space"
            app:keyWidth="30%p" />
        <Key
            app:codes="8230"
            app:keyLabel="…"
            app:keyWidth="15%p" />
        <Key
            app:codes="10"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_return"
            app:keyWidth="20%p" />
    </Row>
</Keyboard>
