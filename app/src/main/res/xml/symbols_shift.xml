<Keyboard xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    app:horizontalGap="1%p"
    app:keyHeight="@dimen/key_height_symbol"
    app:keyWidth="8.8%p"
    app:verticalGap="@dimen/key_vertical_gap_symbol">

    <Row>
        <Key
            app:codes="126"
            app:keyEdgeFlags="left"
            app:keyLabel="~" />
        <Key
            app:codes="177"
            app:keyLabel="±" />
        <Key
            app:codes="215"
            app:keyLabel="×" />
        <Key
            app:codes="247"
            app:keyLabel="÷" />
        <Key
            app:codes="8226"
            app:keyLabel="•" />
        <Key
            app:codes="176"
            app:keyLabel="°" />
        <Key
            app:codes="96"
            app:keyLabel="`" />
        <Key
            app:codes="180"
            app:keyLabel="´" />
        <Key
            app:codes="123"
            app:keyLabel="{" />
        <Key
            app:codes="125"
            app:keyEdgeFlags="right"
            app:keyLabel="}" />
    </Row>

    <Row>
        <Key
            app:codes="169"
            app:keyEdgeFlags="left"
            app:keyLabel="©" />
        <Key
            app:codes="163"
            app:keyLabel="£" />
        <Key
            app:codes="8364"
            app:keyLabel="€" />
        <Key
            app:codes="94"
            app:keyLabel="^" />
        <Key
            app:codes="174"
            app:keyLabel="®" />
        <Key
            app:codes="165"
            app:keyLabel="¥" />
        <Key
            app:codes="95"
            app:keyLabel="_" />
        <Key
            app:codes="43"
            app:keyLabel="+" />
        <Key
            app:codes="91"
            app:keyLabel="[" />
        <Key
            app:codes="93"
            app:keyEdgeFlags="right"
            app:keyLabel="]" />
    </Row>

    <Row>
        <Key
            app:codes="-1"
            app:isModifier="true"
            app:isSticky="true"
            app:keyEdgeFlags="left"
            app:keyIcon="@drawable/ic_key_shift"
            app:keyWidth="13%p" />
        <Key
            app:codes="161"
            app:keyLabel="¡" />
        <Key
            app:codes="60"
            app:keyLabel="&lt;" />
        <Key
            app:codes="62"
            app:keyLabel="&gt;" />
        <Key
            app:codes="162"
            app:keyLabel="¢" />
        <Key
            app:codes="124"
            app:keyLabel="|" />
        <Key
            app:codes="92"
            app:keyLabel="\\" />
        <Key
            app:codes="191"
            app:keyLabel="¿" />
        <Key
            app:codes="-5"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/ic_key_backspace"
            app:keyWidth="14.3%p" />
    </Row>

    <Row
        app:keyHeight="@dimen/key_height_default"
        app:rowEdgeFlags="bottom">
        <Key
            app:codes="-2,-2"
            app:keyEdgeFlags="left"
            app:keyLabel="abc"
            app:keyWidth="13%p" />
        <Key
            app:codes="44"
            app:keyLabel="," />
        <Key
            app:codes="-101"
            app:keyIcon="@drawable/ic_key_language" />
        <Key
            app:codes="32"
            app:isRepeatable="true"
            app:keyWidth="38.4%p" />
        <Key
            app:codes="46"
            app:keyLabel="." />
        <Key
            app:codes="10"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/ic_key_enter"
            app:keyWidth="14.3%p" />
    </Row>
</Keyboard>
